alias ls='ls --color=auto'
alias ll='ls -la'
alias la='ll'
alias clc='clear'
alias cd..='cd ..'
alias valgrind-check='valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes --verbose --log-file=valgrind.out'
alias docker='sudo docker'
